<?xml version="1.0" encoding="utf-8"?>
<form>
	<fields name="filter">
		<field
			name="search"
			type="text"
			label="JSEARCH_FILTER"
			hint="JSEARCH_FILTER"
			class="js-stools-search-string clr"
		/>
		
		<field
			name="order_id"
			type="text"
			hint="COM_ASHS_ORDER_ID"
			label="COM_ASHS_ORDER_ID"
			description="COM_ASHS_ORDER_ID"	
			class="js-stools-search-string clr"		
		/>
		
		<field
			name="order_number"
			type="text"
			hint="COM_ASHS_FORM_ORDER_NUMBER"
			label="COM_ASHS_FORM_ORDER_NUMBER"
			class="clr"
			description="COM_ASHS_FORM_ORDER_NUMBER"			
		/>
		
		<field
			name="product_sku"
			type="productsku"
			multiple="true"	
			class="clr"	
			label="COM_ASHS_ORDER_PRODUCT_SKU"
			description="COM_ASHS_ORDER_PRODUCT_SKU"
					
			>			
		</field>		
		<field
			name="produt_name"
			type="productname"
			multiple="true"
			class="clr"
			label="COM_ASHS_ORDER_PRODUCT_NAME"			
			description="COM_ASHS_ORDER_PRODUCT_NAME"
		
			>
		
		</field>
		
		<field
			name="order_status"
			type="orderstatus"
			multiple="true"
			class="clr"
			label="COM_ASHS_ORDER_ORDER_STATUS"			
			description="COM_ASHS_ORDER_ORDER_STATUS"
			
			>
			
		</field>
		
		<field
			name="payment_method"
			type="paymentmethod"
			multiple="true"
			class="clr"
			label="COM_ASHS_PAYMENT_METHOD"			
			description="COM_ASHS_PAYMENT_METHOD"
			
			>
			
		</field>
		
		<field
			name="email"
			type="text"
			class="clr"
			hint="COM_ASHS_FORM_LBL_ORDER_EMAIL"
			label="COM_ASHS_FORM_LBL_ORDER_EMAIL"			
			description="COM_ASHS_FORM_LBL_ORDER_EMAIL"			
		/>
			
		
		<field
			name="first_name"
			type="text"
			class="clr"
			hint="COM_ASHS_FIRST_NAME"
			label="COM_ASHS_FIRST_NAME"			
			description="COM_ASHS_FIRST_NAME"			
		/>
			
		
		<field
			name="last_name"
			type="text"
			class="clr"
			hint="COM_ASHS_LAST_NAME"
			label="COM_ASHS_LAST_NAME"			
			description="COM_ASHS_LAST_NAME"			
		/>
			

		<field
			name="address"
			type="text"
			class="clr"
			hint="COM_ASHS_ADDRESS"
			label="COM_ASHS_ADDRESS"			
			description="COM_ASHS_ADDRESS"			
		/>
			
		
		<field
			name="city"
			type="text"
			class="clr"
			hint="COM_ASHS_CITY"
			label="COM_ASHS_CITY"			
			description="COM_ASHS_CITY"			
		/>
		<field
			name="state"
			type="text"
			class="clr"
			hint="COM_ASHS_STATE"
			label="COM_ASHS_STATE"			
			description="COM_ASHS_STATE"			
		/>
		
		<field
			name="country"
			type="vmcountry"
			class="clr"
			multiple="true"
			label="COM_ASHS_COUNTRY"			
			description="COM_ASHS_COUNTRY"
			placeholder="COM_ASHS_COUNTRY"
			
			>			
		</field>
		
		<field
			name="zip"
			type="text"
			class="clr"
			hint="COM_ASHS_ZIP"
			label="COM_ASHS_ZIP"			
			description="COM_ASHS_ZIP"			
		/>
		
		<field
			name="include_columnheader"
			type="checkbox"
			class="clr"
			hint="COM_ASHS_COL_HEADER"
			label="COM_ASHS_COL_HEADER"			
			description="COM_ASHS_COL_HEADER"						
		/>
		
		<field
			name="delimiter"
			type="text"
			class="clr"
			hint="COM_ASHS_DELIMITER"
			label="COM_ASHS_DELIMITER"			
			description="COM_ASHS_DELIMITER"						
		/>
		
		<field
			name="enclosure"
			type="text"
			class="clr"
			hint="COM_ASHS_FIELD_ENCLOSURE"
			label="COM_ASHS_FIELD_ENCLOSURE"			
			description="COM_ASHS_FIELD_ENCLOSURE"						
		/>
		
		
